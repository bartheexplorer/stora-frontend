  pending
  cancel
  selesai
  upselling
}

enum t_teks_pesan_status_order {
  cancel
  proses
  kirim
  order
  selesai
}

enum t_bank_is_active {
  NOL @map("0")
  SATU @map("1")
}

enum t_custom_field_type {
  text
  date
  number
  email
  url
}

enum t_log_notif_type {
  app
  link
}

enum t_produk_link_is_active {
  NOL @map("0")
  SATU @map("1")
}

enum t_target_bulan {
  januari
  februari
  maret
  april
  mei
  juni
  juli
  agustus
  september
  oktober
  november
